# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Set default charset
[*]
charset = utf-8

# Indentation settings
[*.{js,jsx,ts,tsx,vue,html,css,scss,sass}]
indent_style = space
indent_size = 2

# Quote settings
[*.{js,jsx,ts,tsx,vue,html,css,scss,sass}]
quote_type = single
